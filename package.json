{
  "name": "discord-choseisan",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "test": "vitest --config vitest.config.node.ts",
    "test:d1": "vitest --config vitest.config.ts",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "deploy": "wrangler deploy && npm run register",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check . && tsc --noEmit",
    "register": "node scripts/register-commands.js",
    "db:migrate:remote": "wrangler d1 migrations apply discord-choseisan-db",
    "db:migrate:local": "wrangler d1 migrations apply discord-choseisan-db --local",
    "db:migrate:create": "wrangler d1 migrations create discord-choseisan-db",
    "db:migrate:list": "wrangler d1 migrations list discord-choseisan-db",
    "db:status": "wrangler d1 execute discord-choseisan-db --command=\"SELECT * FROM d1_migrations ORDER BY version DESC LIMIT 5\"",
    "db:shell": "wrangler d1 execute discord-choseisan-db"
  },
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "discord-interactions": "^4.3.0",
    "hono": "^4.8.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@cloudflare/workers-types": "^4.20250708.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.0.12",
    "@vitest/ui": "^3.2.4",
    "better-sqlite3": "^12.2.0",
    "discord.js": "^14.21.0",
    "dotenv": "^17.1.0",
    "miniflare": "^4.20250705.0",
    "tweetnacl": "^1.0.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.24.0"
  }
}
